# Example environment variables for Angel-X Phase 1 (Angel One integration)
# Copy this file to .env and fill values (do NOT commit real secrets).

# General
STRATEGY_NAME=ANGEL-X
DATA_SOURCE=broker

# Broker / AngelOne credentials (replace with your test/live keys)
ANGELONE_API_KEY=
ANGELONE_CLIENT_CODE=
ANGELONE_PASSWORD=
ANGELONE_TOTP_SECRET=

# Broker websocket / REST
BROKER_WS_URL=wss://broker.example/ws
BROKER_API_KEY=
BROKER_CLIENT_ID=

# Modes
PAPER_TRADING=true
ANALYZER_MODE=true
WEBSOCKET_ENABLED=false

# Trading hours (optional overrides)
MARKET_START_TIME=09:15
MARKET_END_TIME=15:30

# Logging
LOG_LEVEL=INFO

# Safety limits
MAX_DAILY_LOSS_PERCENT=0.03

# NOTES:
# - Keep secrets out of version control. Use a secure vault for production credentials.
# - For testing, you can provide test keys and set PAPER_TRADING=true.


  